<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <!-- Configurations that affect the Test Framework -->
  <RunConfiguration>
    <MaxCpuCount>3</MaxCpuCount>
    <!-- Path relative to solution directory -->
    <!-- [x86] | x64    
      - You can also change it from menu Test, Test Settings, Default Processor Architecture -->
    <TargetPlatform>x64</TargetPlatform>
    <ResultsDirectory>C:\ReleaseAgent\TestResults</ResultsDirectory>
    <!-- Framework35 | [Framework40] | Framework45 -->
    <TargetFrameworkVersion>Framework45</TargetFrameworkVersion>
  </RunConfiguration>
  <TestRunParameters>  
    <Parameter name="BasicLoginTestDataPath" value="INCLUDE_PATH_TO_DATA" />  
    <Parameter name="AlternateLoginTestDataPath" value="INCLUDE_PATH_TO_DATA" />  
  </TestRunParameters> 
</RunSettings>


